[
  {
    "essential": true,
    "image": "${image_url}",
    "name": "${container_name}",
    "portMappings": [
      {
        "containerPort": "${container_port}",
        "hostPort": "${host_port}"
      }
    ],
    "networkMode": "awsvpc",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group_name}",
        "awslogs-region": "${log_group_region}",
        "awslogs-stream-prefix": "${log_group_prefix}"
      }
    }
  }
]